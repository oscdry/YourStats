model Game {
  id         Int    @id @default(autoincrement())
  name       String @db.VarChar(255)
  web_link   String @db.VarChar(255)
  page_views Int    @db.Int
  dev_name   String @db.VarChar(255)
  status     String @db.VarChar(255)
}
